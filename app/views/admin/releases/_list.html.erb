<section class="cards row">
  <% @releases.each do |release| %>
    <div class="card col-md-4">
      <div class="card-image">
        <%= link_to(edit_admin_release_path(release), {}) do %>
          <%= image_tag(release.avatar.jumbo.url) %>
        <% end %>
      </div>
      <div class="card-details">
        <h4><%= release.release_no %></h4>
        <h4><%= release.title %> - <%= release.artist.name %></h4>
      </div>
    </div>
  <% end %>
</section>